{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "bin": {
    "cdk-workshop": "bin/cdk-workshop.js"
  },
  "scripts": {
    "prebuild:app": "trash dist/app",
    "prebuild:cdk": "trash cdk/**/{*.js,*.d.ts}",
    "prebuild:lambda": "trash dist/lambda",
    "build": "run-s build:cdk build:app build:lambda",
    "build:cdk": "tsc",
    "build:app": "ng build",
    "build:lambda": "TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack.lambda.ts",
    "watch:cdk": "tsc -w"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/aws-lambda": "^8.10.19",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.9.2",
    "@types/webpack": "^4.4.24",
    "codelyzer": "~4.5.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "trash-cli": "^1.4.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@aws-cdk/aws-apigateway": "^0.24.1",
    "@aws-cdk/aws-dynamodb": "^0.24.1",
    "@aws-cdk/aws-lambda": "^0.24.1",
    "@aws-cdk/cdk": "^0.24.1",
    "aws-cdk": "^0.24.1",
    "aws-lambda": "^0.1.2",
    "core-js": "^2.5.4",
    "moment": "^2.24.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  }
}
